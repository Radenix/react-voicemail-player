{
  "name": "react-voicemail-player",
  "version": "0.0.4",
  "description": "React component for rendering voice messages in chat-based apps",
  "homepage": "https://github.com/vadimavdeev/react-voicemail-player",
  "main": "dist/react-voicemail-player.cjs.js",
  "module": "dist/react-voicemail-player.esm.js",
  "typings": "typings/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "serve": "esbuild example/index.tsx --bundle --outdir=example/dist --servedir=example/ --serve --watch",
    "start": "concurrently --kill-others --raw \"yarn:serve\" \"yarn:check\"",
    "build": "node ./scripts/build.mjs",
    "test": "jest",
    "check": "tsc --noEmit -w"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:vadimavdeev/react-voicemail-player.git"
  },
  "keywords": [
    "react",
    "voice",
    "voicemail",
    "audio",
    "player"
  ],
  "author": "Vadym Avdieiev",
  "license": "MIT",
  "engines": {
    "node": ">= 14"
  },
  "peerDependencies": {
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react-dom": "^18.2.7",
    "blob-polyfill": "^7.0.20220408",
    "concurrently": "^8.2.0",
    "esbuild": "0.18.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "prettier": "3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6",
    "whatwg-fetch": "^3.6.17"
  },
  "dependencies": {}
